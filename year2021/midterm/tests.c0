void test_q2_copy_stack() {
    result("copy_stack_precond", "invalid stack", 1, false == copy_stack_precond(invalid_stack()));
    result("copy_stack_precond", "empty stack", 1, true == copy_stack_precond(empty_stack()));
    result("copy_stack_precond", "non-empty stack", 1, true == copy_stack_precond(seq(2)));
    result("copy_stack_postcond", "empty stack", 1, true == copy_stack_postcond(empty_stack(), empty_stack()));
    result("copy_stack_postcond", "seq stack", 1, true == copy_stack_postcond(seq(3), seq(2)));
    result("copy_stack_postcond", "non-copy", 1, false == copy_stack_postcond(seq_rev(2,2), seq(2)));
    result("copy_stack", "empty stack", 1, equal_stacks(empty_stack(), copy_stack(empty_stack())));
    result("copy_stack", "seq stack 1", 1, equal_stacks(seq(1), copy_stack(seq(1))));
    result("copy_stack", "seq stack 100", 1, equal_stacks(seq(100), copy_stack(seq(100))));
}


void test_q3_reverse_stack() {
    result("reverse_stack_precond", "invalid stack", 1, false == reverse_stack_precond(invalid_stack(), 1));
    result("reverse_stack_precond", "empty stack but N > 0", 1, false == reverse_stack_precond(empty_stack(), 10));
    result("reverse_stack_precond", "non-empty stack but N > depth", 1, false == reverse_stack_precond(seq(10), 20));
