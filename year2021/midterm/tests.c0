stack_t my_reduce_one(stack_t s) {
    if (stack_depth(s) > 1) {
        int v1 = stack_pop(s);
        int v2 = stack_pop(s);
        stack_push(s, v1+v2);
    }
    return s;
}

stack_t reduce_n(stack_t s, int n) {
    stack_push(s, 0);
    for (int i = 0; i < n; i++) {
        my_reduce_one(s);
    }
    return s;
}

int topval(stack_t s) {
    int val = stack_pop(s);
    stack_push(s, val);
    return val;
}

stack_t sadd(stack_t s, int val) {
    stack_push(s, val);
    return s;
}

void test_q4_reduce_stack() {
    result("reduce_stack_precond", "valid request", 1, true == reduce_stack_precond(seq(10), 10, 10));
    result("reduce_stack_precond", "valid request", 1, true == reduce_stack_precond(seq(10), 5, 10));
    result("reduce_stack_precond", "empty stack", 1, true == reduce_stack_precond(empty_stack(), 0, 0));
    result("reduce_stack_postcond", "invalid stack", 1, false == reduce_stack_postcond(invalid_stack(), 0, 0));
    result("reduce_stack_postcond", "valid stack", 1, true == reduce_stack_postcond(seq(5), 5, 10));
    stack_t s = seq(10);
    reduce_stack(s, 0);
    result("reduce_stack", "zero", 1, equal_stacks(s, seq(10)));
    s = seq(10);
    reduce_stack(s, 1);
    result("reduce_stack", "singleton", 1, equal_stacks(s, seq(10)));
    s = seq(10);
    reduce_stack(s, 3);
    result("reduce_stack", "n values", 1, equal_stacks(s, sadd(seq(7), 240)));
    s = seq(3);
    reduce_one(s);
    result("reduce_one", "one step", 1, topval(s) == 30);
}

