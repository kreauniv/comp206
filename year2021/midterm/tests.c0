    result("reverse_stack_postcond", "empty stack", 1, true == reverse_stack_postcond(empty_stack(), empty_stack(), 0));
    result("reverse_stack_postcond", "short seq stack", 1, true == reverse_stack_postcond(seq_rev(7,7), seq(7), 7));
